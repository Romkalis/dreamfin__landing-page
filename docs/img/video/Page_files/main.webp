@charset "UTF-8";
/* Base */ /* Reset and base styles  */
* {
  padding: 0px;
  margin: 0px;
  border: none;
}

*,
*::before,
*::after {
  box-sizing: border-box;
}

/* Links */
a, a:link, a:visited {
  text-decoration: none;
}

a:hover {
  text-decoration: none;
}

/* Common */
aside, nav, footer, header, section, main {
  display: block;
}

h1, h2, h3, h4, h5, h6, p {
  font-size: inherit;
  font-weight: inherit;
}

ul, ul li {
  list-style: none;
}

img {
  vertical-align: top;
}

img, svg {
  max-width: 100%;
  height: auto;
}

address {
  font-style: normal;
}

/* Form */
input, textarea, button, select {
  font-family: inherit;
  font-size: inherit;
  color: inherit;
  background-color: transparent;
}

input::-ms-clear {
  display: none;
}

button, input[type=submit] {
  display: inline-block;
  box-shadow: none;
  background-color: transparent;
  background: none;
  cursor: pointer;
}

input:focus, input:active,
button:focus, button:active {
  outline: none;
}

button::-moz-focus-inner {
  padding: 0;
  border: 0;
}

label {
  cursor: pointer;
}

legend {
  display: block;
}

:root {
  --c-light: #fff;
  --text-black: #000;
  --accent-light: #E6D5C1;
  --accent-dark: #CC9B7C;
  --text-dark: #020202;
  --c-main: #380017;
  --text-c-medium: #412E36;
}

@font-face {
  font-family: "Open Sans";
  font-display: swap;
  src: local("Open Sans"), url("../fonts/OpenSans.woff2") format("woff2"), url("../fonts/OpenSans.woff") format("woff"), url("../fonts/OpenSans.ttf") format("truetype");
  font-weight: 400, 500, 600, 700, 800;
  font-style: normal;
}
@font-face {
  font-family: "Open Sans Italic";
  font-display: swap;
  src: url("../fonts/OpenSans-Italic.woff2") format("woff2"), url("../fonts/OpenSans-Italic.woff") format("woff"), url("../fonts/OpenSans-Italic.ttf") format("truetype");
  font-weight: 400, 500, 600, 700, 800;
  font-style: italic;
}
.visually-hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  border: 0;
  padding: 0;
  white-space: nowrap;
  clip-path: inset(100%);
  clip: rect(0 0 0 0);
  overflow: hidden;
}

html {
  scroll-behavior: smooth;
  background-color: rgb(255, 255, 255);
  font-family: "Open Sans", sans-serif;
}

html, body {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}

.footer {
  margin-top: auto;
}

.footer {
  padding: 60px 0;
  background-color: #e3e3e3;
}

.container {
  box-sizing: border-box;
  width: 100%;
  max-width: 1600px;
  padding: 0 40px;
  margin: 0 auto;
}
@media (max-width: --mobile) {
  .container {
    padding: 0 10px;
  }
}

/* Blocks */
.button {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15px 34px;
  flex: 1 0 0;
  transition: all 0.4s ease-in-out;
}
.button--dark {
  border-radius: 50px;
  background: var(--c-main);
  color: var(--accent-light);
  font-family: "Open Sans";
  font-size: 16px;
  font-style: normal;
  font-weight: 700;
  line-height: 125%; /* 20px */
  text-transform: uppercase;
  border: 2px solid var(--c-main);
}
.button:hover {
  border: 2px solid var(--c-main);
  background-color: var(--c-light);
  color: var(--c-main);
}
.button:active {
  border: 2px solid var(--accent-light);
  background-color: var(--accent-light);
  color: var(--c-main);
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 20px;
  padding-bottom: 30px;
}

.header__logo {
  width: 310px;
  height: auto;
}

.header__nav {
  display: flex;
  flex: 1 1 0;
  align-items: center;
}

.header__navlist {
  display: flex;
  flex-wrap: wrap;
  column-gap: 30px;
  margin: 0 auto;
  padding: 10px 20px;
  justify-content: center;
}

.header__contacts {
  display: flex;
  flex-wrap: wrap;
  gap: 20px;
  align-items: center;
  justify-content: center;
  column-gap: 30px;
}

.header__contacts-phone {
  font-size: 16px;
  line-height: 140%; /* 22.4px */
}

.header__button {
  padding: 15px clamp(2rem, 133px, 9rem);
}
.header__button span:first-child {
  display: none;
}

.header__burger-checkbox {
  display: none;
}

.header__burger, .header__contacts-mail, .header__social {
  display: none;
}

@media (min-width: 469px) and (max-width: 768px) {
  .header {
    display: flex;
    flex-wrap: wrap;
  }
  .header__logo {
    width: 100%;
    max-width: 20vw;
  }
  .header__button {
    padding: 10px 20px;
  }
  .header__navlist {
    row-gap: 10px;
    justify-content: space-between;
  }
}
@media (max-width: 468px) {
  .header {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-template-rows: auto auto;
    column-gap: 10px;
    row-gap: 20px;
    padding-top: 10px;
    margin-bottom: 40px;
    position: relative;
  }
  .header__logo {
    grid-column: 1/2;
    grid-row: 1/2;
    justify-self: start;
    width: 30vw;
  }
  .header__burger {
    display: block;
    grid-column: 2/3;
    grid-row: 1/2;
    justify-self: end;
    margin-left: auto;
    width: 24px;
    height: 16px;
    position: relative;
    cursor: pointer;
    padding: 0 0 10px 20px;
  }
  .header__burger::before, .header__burger::after {
    content: "";
    left: 0;
    position: absolute;
    display: block;
    width: 100%;
    height: 1px;
    background: #000;
  }
  .header__burger::before {
    top: 3px;
    box-shadow: 0 5px 0 #000;
    transition: box-shadow 0.3s 0.15s, top 0.3s 0.15s, transform 0.3s;
  }
  .header__burger::after {
    bottom: 2px;
    transition: bottom 0.3s 0.15s, transform 0.3s;
  }
  .header__burger-checkbox:checked + .header__burger::before {
    top: 8px;
    transform: rotate(40deg);
    box-shadow: 0 6px 0 rgba(0, 0, 0, 0);
    background-color: var(--accent-dark);
    transition: box-shadow 0.15s, top 0.3s, transform 0.3s 0.15s;
  }
  .header__burger-checkbox:checked + .header__burger::after {
    bottom: 7px;
    transform: rotate(-40deg);
    background-color: var(--accent-dark);
    transition: bottom 0.3s, transform 0.3s 0.15s;
  }
  .header__nav {
    grid-column: 1/-1;
    grid-row: 2/3;
    height: 0;
    overflow: hidden;
    position: absolute;
    top: 20px;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    background: var(--c-light);
    width: 100vw;
    border-radius: 0px 0px 50px 50px;
    row-gap: 40px;
  }
  .header__burger-checkbox:checked ~ .header__nav {
    height: auto;
  }
  .header__navlist {
    flex-direction: column;
    text-align: center;
    color: var(--text-c-medium);
    text-align: center;
    font-size: 16px;
    font-style: normal;
  }
  .header__navitem {
    margin-bottom: 5px;
  }
  .header__navlink {
    line-height: 140%;
    /* 19.6px */
    color: var(--text-c-medium);
  }
  .header__contacts {
    display: flex;
    flex-direction: column;
  }
  .header__contacts-phone {
    order: 2;
    color: var(--c-main);
    text-align: center;
    font-size: 21px;
    line-height: 135%;
    margin-bottom: 10px;
  }
  .header__button {
    order: 1;
    margin-bottom: 20px;
    width: 100%;
    padding: 15px 34px;
  }
  .header__button span:first-child {
    display: block;
  }
  .header__button span:nth-child(2) {
    display: none;
  }
  .header__contacts-mail {
    display: block;
    order: 3;
    color: var(--c-main);
    text-align: center;
    font-size: 21px;
    line-height: 135%;
  }
  .header__social {
    display: flex;
    column-gap: 30px;
    margin-bottom: 40px;
  }
  .header__social-icon:first-child {
    width: 48px;
    height: 48px;
    background-image: url("../img/svgicons/social-telegram.svg");
    mask: url("../img/svgicons/social-telegram.svg") no-repeat center;
  }
  .header__social-icon:nth-child(2) {
    width: 48px;
    height: 48px;
    background-image: url("../img/svgicons/social-vk.svg");
    mask: url("../img/svgicons/social-vk.svg") no-repeat center;
  }
  .header__social-icon:nth-child(3) {
    width: 48px;
    height: 48px;
    background-image: url("../img/svgicons/social-round.svg");
    mask: url("../img/svgicons/social-round.svg") no-repeat center;
    background-repeat: no-repeat;
    position: relative;
  }
  .header__social-icon:nth-child(3)::after {
    content: "IN";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: var(--c-light);
    font-size: 20px;
    font-weight: 700;
  }
}
.link {
  color: var(--text-dark);
  font-family: "Open Sans";
  font-style: normal;
  font-weight: 400;
  line-height: 140%;
  transition: all 0.3s ease-in;
  position: relative;
}
.link::before {
  position: absolute;
  content: "";
  bottom: -0.1em;
  left: 50%;
  transform: translateX(-50%);
  width: 0%;
  height: 0.1em;
  background-color: var(--c-main);
  transition: width 0.3s ease-in-out;
}
.link:hover {
  color: var(--c-main);
}
.link:hover::before {
  width: 100%;
}
.link:active {
  color: var(--c-main);
  font-weight: 600;
}

